page.call "crm.flip_form", :create_stock_product

unless params[:cancel].true?
  page[:create_stock_product].replace_html :partial => "new"
  page[:empty].replace_html ""
  page.call "crm.set_title", :create_stock_product, t(:create_stock_product)
else
  page.call "crm.set_title", :create_stock_product, t(:stock_products)
end
