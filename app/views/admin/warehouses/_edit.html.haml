.remote
  = form_for([:admin, @warehouse], :html => one_submit_only(:warehouse).merge({:autocomplete => 'off'}), :remote => true) do |f|
    = link_to_close edit_admin_warehouse_path(@warehouse)

    = f.error_messages :object_name => t('warehouse')

    = render "admin/warehouses/form", :f => f, :edit => true

    .buttonbar
      = f.submit t(:save_warehouse)
      #{t :or}
      = link_to_cancel edit_admin_warehouse_path(@warehouse)
